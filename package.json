{
  "name": "@neoncity/identity",
  "version": "0.0.23",
  "description": "The identity service.",
  "author": "NeonCity team",
  "scripts": {
    "build": "tsc",
    "serve": "node out/src/index.js",
    "pretest": "tsc",
    "test": "mocha out/tests/index.js",
    "test-with-coverage": "tsc && istanbul cover --config ./.istanbul.yml ./node_modules/mocha/bin/_mocha -- out/tests/index.js && remap-istanbul -i ./out/coverage/coverage-final.json -o ./out/coverage/coverage-remapped.json && coverage-filter -i ./out/coverage/coverage-remapped.json -o ./out/coverage/coverage-filtered.json",
    "push-coverage-to-codecov": "codecov -f ./out/coverage/coverage-filtered.json"
  },
  "respository": {
    "type": "git",
    "url": "git+https://github.com/neoncity/identity.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://github.com/neoncity/identity#readme",
  "filesPack": {
    "out": "e:.",
    "migrations": "c:migrations",
    "Procfile": "f:Procfile",
    "knexfile.js": "f:knexfile.js",
    "package.json": "f:package.json",
    "tsconfig.json": "f:tsconfig.json"
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.12.6",
    "pg": "^6.1.2"
  },
  "devDependencies": {
    "@neoncity/common-js": "^0.0.11",
    "@neoncity/coverage-filter": "^0.0.5",
    "@neoncity/togemfury": "^0.0.9",
    "@types/chai": "^3.4.34",
    "@types/es6-promise": "0.0.32",
    "@types/express": "^4.0.34",
    "@types/isomorphic-fetch": "0.0.31",
    "@types/knex": "0.0.39",
    "@types/mocha": "^2.2.37",
    "@types/node": "^7.0.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "remap-istanbul": "^0.8.4",
    "typescript": "^2.1.5"
  }
}
