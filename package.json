  "description": "The identity service.",
  "author": "NeonCity team",
  "scripts": {
    "build": "webpack",
    "serve": "node out/app.js",
    "pretest": "webpack",
    "test": "mocha out/tests.js",
    "test-with-coverage": "webpack && istanbul cover --config ./.istanbul.yml ./node_modules/mocha/bin/_mocha -- out/tests.js && remap-istanbul -i ./out/coverage/coverage-final.json -o ./out/coverage/coverage-remapped.json && node ./bin/coverage-filter.js",
    "push-coverage-to-codecov": "codecov -f ./out/coverage/coverage-filtered.json"
  },
  "respository": {
    "type": "git",
    "url": "git+https://github.com/neoncity/identity.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://github.com/neoncity/identity#readme",
  "files": [
    "out",
    "src",
    "Procfile",
    "webpack.config.js"
  ],
  "dependencies": {
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.12.6",
    "pg": "^6.1.2"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/es6-promise": "0.0.32",
    "@types/express": "^4.0.34",
    "@types/isomorphic-fetch": "0.0.31",
    "@types/knex": "0.0.39",
    "@types/mocha": "^2.2.37",
    "@types/node": "^7.0.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "remap-istanbul": "^0.8.4",
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.5",
    "webpack": "^1.13.2",
    "webpack-fail-plugin": "^1.0.5"
  }
}
